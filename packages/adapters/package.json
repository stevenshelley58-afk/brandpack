{
  "name": "@brandpack/adapters",
  "version": "0.1.0",
  "private": true,
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./anthropic": {
      "types": "./dist/anthropic.d.ts",
      "default": "./dist/anthropic.js"
    },
    "./openai": {
      "types": "./dist/openai.d.ts",
      "default": "./dist/openai.js"
    },
    "./noop": {
      "types": "./dist/noop.d.ts",
      "default": "./dist/noop.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "type-check": "tsc --noEmit",
    "test": "vitest run"
  },
  "dependencies": {
    "@brandpack/core": "workspace:*",
    "@anthropic-ai/sdk": "^0.27.0",
    "openai": "^4.52.0"
  },
  "devDependencies": {
    "typescript": "^5.3.3",
    "@types/node": "^20.10.6",
    "vitest": "^1.6.0"
  }
}

